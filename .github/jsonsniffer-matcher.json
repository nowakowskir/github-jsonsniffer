{
  "problemMatcher": [
    {
      "owner": "jsonsniffer",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^\\s].*)$",
          "file": 1
        },
        {
          "regexp": "^(error|warning|info)\\s+(.*)$",
          "severity": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}